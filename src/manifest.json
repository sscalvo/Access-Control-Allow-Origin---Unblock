{
  "manifest_version": 2,
  "version": "0.1.6",
  "name": "Pikai manager",
  "description": "Multi-user dataset labeler manager" ,
  "permissions": [
    "storage",
	"<all_urls>",
    "webRequest",
    "webRequestBlocking",
    "contextMenus"
  ],
  "icons": {
    "16": "data/icons/16.png",
    "19": "data/icons/19.png",
    "32": "data/icons/32.png",
    "38": "data/icons/38.png",
    "48": "data/icons/48.png",
    "64": "data/icons/64.png",
    "128": "data/icons/128.png",
    "256": "data/icons/256.png",
    "512": "data/icons/512.png"
  },
  "homepage_url": "https://pikai.sscalvo.com",
  "browser_action":{},
  "background": {
    "scripts": [
      "background.js"
    ]
  }
}
